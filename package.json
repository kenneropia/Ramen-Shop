{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env nodemon server.js",
    "start-big": "nodemon --max-old-space-size=2096 server.js",
    "start:prod": "nodemon server.js --NODE_ENV:production",
    "debug": "ndb server.js",
    "watch:js": "parcel watch public/js/index.js --out-dir public/js --out-file bundle.js --no-source-maps",
    "build:js": "parcel build public/js/index.js --out-dir public/js --out-file bundle.js --no-source-maps",
    "db": "mongod"
  },
  "author": "Me",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/express": "^4.17.9",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^3.5.0",
    "helmet": "^3.16.0",
    "hpp": "^0.2.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.1.1",
    "pug": "^3.0.2",
    "sharp": "^0.22.1",
    "slugify": "^1.4.6",
    "stripe": "^7.0.0",
    "validator": "^10.11.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": "^12"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
